		"wp-pay-gateways/qantani-mollie": "^1.0.1",
		"wp-pay-gateways/rabobank-ideal-professional-v3": "^1.0.1",
		"wp-pay-gateways/sisow": "^1.2.2",
		"wp-pay-gateways/sisow-ideal-basic": "^1.0.0",
